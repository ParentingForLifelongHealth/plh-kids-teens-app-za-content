{
  "==content_list==": [
    {
      "flow_type": "data_list",
      "flow_name": "launch_actions",
      "status": "draft",
      "flow_subtype": "lifecycle_actions",
      "comment": "This workbook potentially has become redundant after implementation of the PLH onboarding library workbook"
    },
    {
      "flow_type": "template",
      "flow_name": "splash_footer",
      "status": "draft",
      "comment": "PLH onboarding"
    },
    {
      "flow_type": "template",
      "flow_name": "user_data_splash",
      "status": "draft"
    },
    {
      "flow_type": "template",
      "flow_name": "login_splash",
      "status": "draft"
    },
    {
      "flow_type": "template",
      "flow_name": "language_select_splash",
      "status": "draft"
    }
  ],
  "launch_actions": [
    {
      "lifecycle_event": "app_start",
      "id": "app_start_update_complete",
      "priority": 1,
      "condition_list": "@fields._app_update_downloaded",
      "action_list": "app_update: complete"
    },
    {
      "lifecycle_event": "app_start",
      "id": "app_start_language_select",
      "priority": 2,
      "condition_list": "!@fields.language_selected && @global.has_multiple_languages && @fields.user_mode",
      "action_list": "pop_up: language_select_splash | fullscreen: true, showCloseButton: false"
    },
    {
      "lifecycle_event": "app_start",
      "id": "app_start_accept_terms",
      "priority": 3,
      "condition_list": "!@fields.@global.deployment_terms_field && @fields.user_mode",
      "action_list": "pop_up: accept_terms | fullscreen: true, showCloseButton: false"
    },
    {
      "lifecycle_event": "app_start",
      "id": "app_start_login",
      "priority": 4,
      "condition_list": "!@fields.login_skipped && !@fields._auth_user_id && @global.has_optional_login && @fields.user_mode",
      "action_list": "pop_up: login_splash | fullscreen: true, showCloseButton: false"
    },
    {
      "lifecycle_event": "app_start",
      "id": "app_start_user_data",
      "priority": 5,
      "condition_list": "!@fields.user_data_completed && @fields.user_mode",
      "action_list": "pop_up: user_data_splash | fullscreen: true, showCloseButton: false"
    },
    {
      "lifecycle_event": "app_start",
      "id": "app_start_splash_screens_completed",
      "priority": 6,
      "condition_list": "!@fields.splash_screens_completed  && @fields.user_mode",
      "action_list": "set_field: splash_screens_completed: true"
    },
    {
      "lifecycle_event": "app_start",
      "id": "app_start_update_prompt",
      "priority": 7,
      "action_list": "app_update: prompt"
    }
  ],
  "splash_footer": [
    {
      "type": "begin_display_group",
      "name": "dg_bottom",
      "parameter_list": "sticky: bottom;\nbackground_image_asset: images/backgrounds/dunes.png;\nbackground_image_position: bottom left;",
      "exclude_from_translation": true,
      "style_list": "min-height: 192px;\nwidth: 110%;"
    },
    {
      "type": "end_display_group"
    }
  ],
  "user_data_splash": [
    {
      "type": "begin_template",
      "name": "user_data",
      "value": "user_data",
      "action_list": "completed | emit: completed"
    },
    {
      "type": "end_template"
    },
    {
      "type": "begin_template",
      "name": "splash_footer",
      "value": "splash_footer"
    },
    {
      "type": "end_template"
    }
  ],
  "login_splash": [
    {
      "type": "begin_template",
      "name": "login",
      "value": "login",
      "action_list": "completed | emit: completed"
    },
    {
      "type": "end_template"
    },
    {
      "type": "begin_template",
      "name": "splash_footer",
      "value": "splash_footer"
    },
    {
      "type": "end_template"
    }
  ],
  "language_select_splash": [
    {
      "type": "begin_template",
      "name": "language_select",
      "value": "language_select",
      "action_list": "completed | emit: completed"
    },
    {
      "type": "end_template"
    },
    {
      "type": "begin_template",
      "name": "splash_footer",
      "value": "splash_footer"
    },
    {
      "type": "end_template"
    }
  ]
}