{
  "==content_list==": [
    {
      "flow_type": "data_pipe",
      "flow_name": "prep_modules",
      "status": "released"
    },
    {
      "flow_type": "generator",
      "flow_name": "gen_prep_categories",
      "status": "released",
      "parameter_list": "input_data_list: modules;\noutput_flow_name: prep_{@gen.id}_categories;\noutput_flow_type: data_pipe;"
    },
    {
      "flow_type": "generator",
      "flow_name": "gen_prep_articles",
      "status": "released",
      "parameter_list": "input_data_list: modules;\noutput_flow_name: prep_{@gen.id}_articles;\noutput_flow_type: data_pipe;"
    }
  ],
  "prep_modules": [
    {
      "operation": "append_columns",
      "args_list": "completed: FALSE;\ntask_child: {@row.id}_article_tasks;",
      "input_source": "modules",
      "output_target": "module_tasks"
    }
  ],
  "gen_prep_categories": [
    {
      "operation": "append_columns",
      "input_source": "common_categories",
      "output_target": "{@gen.id}_category_tasks"
    }
  ],
  "gen_prep_articles": [
    {
      "operation": "filter",
      "args_list": "id == \"@gen.relax_id\"",
      "input_source": "relax",
      "comment": "Take relax article from separate data list"
    },
    {
      "operation": "append_columns",
      "args_list": "category_id: relax;\nnested: FALSE;",
      "output_target": "{@gen.id}_relax | local_only;"
    },
    {
      "operation": "filter",
      "args_list": "id == \"@gen.check_in_id\"",
      "input_source": "check_in",
      "comment": "Take check-in article from separate data list"
    },
    {
      "operation": "append_columns",
      "args_list": "category_id: check_in;\nnested: FALSE;",
      "output_target": "{@gen.id}_check_in | local_only;"
    },
    {
      "operation": "filter",
      "args_list": "!nested",
      "input_source": "{@gen.id}_articles",
      "output_target": "{@gen.id}_testing | local_only;"
    },
    {
      "operation": "concat",
      "args_list": "{@gen.id}_relax;\n{@gen.id}_check_in;\n{@gen.id}_testing;",
      "input_source": false
    },
    {
      "operation": "append_columns",
      "args_list": "completed: FALSE",
      "output_target": "{@gen.id}_article_tasks"
    }
  ],
