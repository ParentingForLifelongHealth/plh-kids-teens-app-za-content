  "onboarding_header": [
    {
      "type": "begin_display_group",
      "name": "dg_top",
      "parameter_list": "sticky: top;",
      "style_list": "background-color: @global.onboarding_header_background_colour;\nborder-bottom-left-radius: 36px;\nborder-bottom-right-radius: 36px;\nmargin-block: -24px;"
    },
    {
      "type": "image",
      "name": "logo",
      "value": "@global.onboarding_header_image_asset",
      "style_list": "height: @global.onboarding_header_image_height;\nmargin-top: 48px;\nmargin-bottom: 24px;"
    },
    {
      "type": "end_display_group"
    },
    {
      "type": "text",
      "name": "spacer",
      "style_list": "margin-top: 24px"
    }
  ],
  "onboarding_footer": [
    {
      "type": "begin_display_group",
      "name": "dg_bottom",
      "parameter_list": "sticky: bottom;\nbackground_image_asset: @global.onboarding_footer_image_asset;\nbackground_image_position: bottom left;",
      "style_list": "aspect-ratio: 1;\nwidth: 100%;\npointer-events: none;"
    },
    {
      "type": "end_display_group"
    }
