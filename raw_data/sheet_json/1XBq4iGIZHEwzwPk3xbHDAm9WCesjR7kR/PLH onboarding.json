    },
    {
      "name": "language_select_options",
      "value": "@data.language"
    },
    {
      "type": "radio_button_grid",
      "name": "language_grid",
      "action_list": "changed | emit: set_language: this.value;\nchanged | emit: completed;\nchanged | set_field: onboarding_language_select_completed: true;",
      "parameter_list": "answer_list: @local.language_select_options; \nitem_width: 200px;"
    },
    {
      "type": "begin_template",
      "name": "footer",
      "value": "onboarding_footer",
      "condition": "!!@global.onboarding_footer_image_asset"
    },
    {
      "type": "end_template"
    }
  ],
  "onboarding_splash_slides": [],
  "onboarding_splash_slide": [
    {
      "name": "show_back",
      "value": true
